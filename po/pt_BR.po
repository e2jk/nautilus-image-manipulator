#: ../nautilus_image_manipulator/NautilusImageManipulatorDialog.py:90
msgid "Please enter a value for the subdirectory."
msgstr "Por favor, insira um valor para o subdiretório."

#: ../nautilus_image_manipulator/NautilusImageManipulatorDialog.py:97
msgid "Please enter some text to append to the filename."
msgstr ""
"Por favor, insira algum texto para adicionar ao final do nome de arquivo."

#: ../nautilus_image_manipulator/NautilusImageManipulatorDialog.py:103
#, python-format
msgid "The string to append cannot end in %s"
msgstr "A frase a ser adicionada não pode terminar em %s"

#: ../nautilus_image_manipulator/NautilusImageManipulatorDialog.py:137
#: ../nautilus_image_manipulator/ImageManipulations.py:77
msgid "Resizing images..."
msgstr "Redimensionando imagens..."

#: ../nautilus_image_manipulator/NautilusImageManipulatorDialog.py:188
#: ../nautilus_image_manipulator/NautilusImageManipulatorDialog.py:199
#, python-format
msgid ""
"Your images have not been sent, but have been zipped together into this "
"file:\n"
"%(filename)s"
msgstr ""
"Suas imagens não foram enviadas, contudo, elas foram compactadas neste "
"arquivo:\n"
"%(filename)s"

#: ../nautilus_image_manipulator/NautilusImageManipulatorDialog.py:190
#: ../nautilus_image_manipulator/NautilusImageManipulatorDialog.py:201
#, python-format
msgid ""
"Your image has not been sent, but has successfully been resized.\n"
"You can find it at %(filename)s"
msgstr ""
"Sua imagem não foi enviada, contudo, foi redimensionada com sucesso.\n"
"Você pode encontrá-la em %(filename)s"

#: ../nautilus_image_manipulator/NautilusImageManipulatorDialog.py:191
#, python-format
msgid "The selected upload site %(site_name)s is not valid."
msgstr "O website selecionado para envio %(site_name)s não é válido."

#: ../nautilus_image_manipulator/NautilusImageManipulatorDialog.py:191
msgid "Please file a bug report on Launchpad"
msgstr "Por favor, abra uma notificação de bug no Launchpad"

#: ../nautilus_image_manipulator/NautilusImageManipulatorDialog.py:202
#, python-format
msgid ""
"The upload site %(site_name)s could not be contacted, please check your "
"internet connection."
msgstr ""
"O website de envio %(site_name)s não pôde ser contactado, por favor, "
"verifique a sua conexão de internet."

#: ../nautilus_image_manipulator/NautilusImageManipulatorDialog.py:205
#: ../nautilus_image_manipulator/NautilusImageManipulatorDialog.py:254
msgid "Uploading images..."
msgstr "Fazendo upload das imagens..."

#: ../nautilus_image_manipulator/NautilusImageManipulatorDialog.py:341
msgid "Invalid parameters"
msgstr "Parâmetros inválidos"

#: ../nautilus_image_manipulator/NautilusImageManipulatorDialog.py:355
msgid "_Skip"
msgstr "_Ignorar"

#: ../nautilus_image_manipulator/NautilusImageManipulatorDialog.py:357
msgid "_Retry"
msgstr "_Repetir"

#: ../nautilus_image_manipulator/NautilusImageManipulatorDialog.py:358
#, python-format
msgid ""
"The image \"%(image)s\" could not be resized.\n"
"\n"
"Check whether you have permission to write to this folder:\n"
"%(folder)s"
msgstr ""
"A imagem \"%(image)s\" não pôde ser redimensionada.\n"
"\n"
"Verifique se você possui permissão de escrita  nesta pasta:\n"
"%(folder)s"

#: ../nautilus_image_manipulator/NautilusImageManipulatorDialog.py:360
msgid "Could not resize image"
msgstr "Não foi possível redimensionar a imagem"

#. Default name of the subdirectory in which the resized images will be put
#: ../nautilus_image_manipulator/NautilusImageManipulatorDialog.py:423
msgid "resized"
msgstr "redimensionado"

#. Default value of the string that will be appended to the filename of the resized images
#: ../nautilus_image_manipulator/NautilusImageManipulatorDialog.py:425
msgid "-resized"
msgstr "-redimensionado"

#: ../bin/nautilus-image-manipulator.py:45
msgid "show debug messages"
msgstr "exibir mensagens de debug"

#: ../bin/nautilus-image-manipulator.py:46
msgid ""
"a file to be resized. Use multiple arguments to resize multiple files."
msgstr ""
"um arquivo para ser redimensionado. Use argumentos múltiplos para "
"redimensionar múltiplos arquivos."

#: ../bin/nautilus-image-manipulator.py:66
#: ../nautilus_image_manipulator/nautilus-extension.py:60
msgid "_Resize image"
msgid_plural "_Resize images"
msgstr[0] "_Redimensionar imagem"
msgstr[1] "_Redimensionar imagens"

#. Display an error message since we don't have images to work on
#: ../bin/nautilus-image-manipulator.py:68
#, python-format
msgid ""
"Nautilus Image Manipulator needs to be provided with a list of images to "
"resize.\n"
"Please right-click on an image and select the \"%(resizeImages)s\" option."
msgstr ""
"O \"Manipulador de Imagens do Nautilus\" necessita que uma lista de imagens "
"(a serem redimensionadas) seja fornecida.\n"
"Por favor, clique com o botão direito do mouse em uma imagem e selecione a "
"opção \"%(resizeImages)s\"."

#: ../bin/nautilus-image-manipulator.py:70
msgid "No images provided"
msgstr "Nenhuma imagem foi fornecida"

#: ../nautilus_image_manipulator/ImageManipulations.py:219
msgid "Packing images..."
msgstr "Criando pacote com imagens..."

#: ../data/ui/NautilusImageManipulatorDialog.ui.h:1
msgid "An error occurred!"
msgstr "Ocorreu um erro!"

#. Part of "Append XX to file name"
#: ../data/ui/NautilusImageManipulatorDialog.ui.h:3
msgid "Append"
msgstr "Anexar"

#: ../data/ui/NautilusImageManipulatorDialog.ui.h:4
msgid "Attach to an email message"
msgstr "Anexar a um e-mail"

#: ../data/ui/NautilusImageManipulatorDialog.ui.h:5
msgid "Compression:"
msgstr "Compressão:"

#: ../data/ui/NautilusImageManipulatorDialog.ui.h:6
msgid "Custom size:"
msgstr "Tamanho personalizado:"

#: ../data/ui/NautilusImageManipulatorDialog.ui.h:7
msgid "Delete url:"
msgstr "Apagar url:"

#: ../data/ui/NautilusImageManipulatorDialog.ui.h:8
msgid "Download url:"
msgstr "Endereço de download:"

#: ../data/ui/NautilusImageManipulatorDialog.ui.h:9
msgid "Force aspect ratio"
msgstr "Manter proporções"

#: ../data/ui/NautilusImageManipulatorDialog.ui.h:10
msgid "Height:"
msgstr "Altura:"

#. Use "Title Case" if used in your language
#: ../data/ui/NautilusImageManipulatorDialog.ui.h:12
msgid "Image Size"
msgstr "Tamanho da Imagem"

#: ../data/ui/NautilusImageManipulatorDialog.ui.h:13
msgid "Images successfully uploaded!"
msgstr "As imagens foram enviadas com sucesso!"

#. Use "Title Case" if used in your language
#: ../data/ui/NautilusImageManipulatorDialog.ui.h:15
msgid "Output Options"
msgstr "Opções de Saída"

#: ../data/ui/NautilusImageManipulatorDialog.ui.h:16
msgid "Put in subdirectory called"
msgstr "Colocar no sub-diretório"

#: ../data/ui/NautilusImageManipulatorDialog.ui.h:17
msgid "Resize"
msgstr "Redimensionar"

#: ../data/ui/NautilusImageManipulatorDialog.ui.h:18
msgid "Resize images"
msgstr "Redimensionar imagens"

#: ../data/ui/NautilusImageManipulatorDialog.ui.h:19
msgid "Resize in place"
msgstr "Redimensionar e substituir no mesmo arquivo"

#: ../data/ui/NautilusImageManipulatorDialog.ui.h:20
msgid "Scale:"
msgstr "Escala:"

#: ../data/ui/NautilusImageManipulatorDialog.ui.h:21
msgid "Select a size:"
msgstr "Selecione um tamanho:"

#: ../data/ui/NautilusImageManipulatorDialog.ui.h:22
msgid "Send the resized images"
msgstr "Enviar as imagens redimensionadas"

#. Use "Title Case" if used in your language
#: ../data/ui/NautilusImageManipulatorDialog.ui.h:24
msgid "Sending Options"
msgstr "Opções de Envio"

#: ../data/ui/NautilusImageManipulatorDialog.ui.h:25
msgid "The download url has been copied in the clipboard"
msgstr "O endereço de download foi copiado para área de transferência"

#: ../data/ui/NautilusImageManipulatorDialog.ui.h:26
msgid "This value will be appended before the image extension"
msgstr "Este valor será anexado antes da extensão da imagem"

#. After this text comes a combobox to select to which website to upload the images to. It will thus be like "Upload a .zip file to example.com"
#: ../data/ui/NautilusImageManipulatorDialog.ui.h:28
msgid "Upload a .zip file to"
msgstr "Enviar um arquivo .zip para"

#: ../data/ui/NautilusImageManipulatorDialog.ui.h:29
msgid "Width:"
msgstr "Largura:"

#: ../data/ui/NautilusImageManipulatorDialog.ui.h:30
msgid "percent"
msgstr "porcento"

#: ../data/ui/NautilusImageManipulatorDialog.ui.h:31
msgid "pixels"
msgstr "pixels"

#. Part of "Append XX to file name"
#: ../data/ui/NautilusImageManipulatorDialog.ui.h:33
msgid "to file name"
msgstr "para o nome do arquivo"

#: ../nautilus_image_manipulator/nautilus-extension.py:61
msgid "Resize the selected image"
msgid_plural "Resize each selected image"
msgstr[0] "Redimensionar a imagem selecionada"
msgstr[1] "Redimensionar as imagens selecionadas"
